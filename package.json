  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:cov": "react-app-rewired test --coverage --watchAll=false",
    "echo-version": "echo version=$npm_package_version",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx} --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "plugins": [
      "security"
    ],
    "extends": [
      "react-app",
      "plugin:security/recommended"
    ],
    "globals": {
      "config": "readonly"
    },
    "rules": {
      "react/no-unescaped-entities": "warn",
      "react/no-danger": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/react-custom-scrollbars": "4.0.7",
    "@types/react-file-icon": "^1.0.1",
    "@types/react-google-recaptcha": "^2.1.4",
    "@types/react-virtualized": "9.21.11",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "eslint-plugin-security": "^1.4.0",
    "jest-mock": "^27.4.6",
    "react-app-rewired": "^2.2.1",
    "ts-jest": "^27.1.2"
  },
  "jest": {
    "moduleNameMapper": {
      "react-bootstrap/esm/(.*)": "<rootDir>/node_modules/react-bootstrap/cjs/$1.js"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/reducers",
      "<rootDir>/src/test"
    ]
  }
}
